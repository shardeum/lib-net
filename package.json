{
  "name": "shardus-net",
  "version": "1.0.2",
  "description": "",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "src/**/*"
  ],
  "scripts": {
    "build": "npm run build-rust-release && npm run build-node",
    "build-node": "tsc -p .",
    "build-rust": "cargo-cp-artifact -nc shardus-net.node -- cargo build --message-format=json-render-diagnostics",
    "build-rust-debug": "npm run build-rust --",
    "build-rust-release": "npm run build-rust -- --release",
    "prepare": "npm run build",
    "install": "npm run build",
    "test": "cargo test"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/Shardus/shardus-net.git"
  },
  "contributors": [
    "Aaron Sullivan",
    "Derrick Farris",
    "Aamir Syed",
    "Kyle Shifflett",
    "James Harmon"
  ],
  "dependencies": {
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.0",
    "@types/uuid": "^7.0.0",
    "cargo-cp-artifact": "^0.1",
    "gts": "^3.1.0",
    "typescript": "4.4.4"
  }
}
